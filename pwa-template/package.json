{
  "name": "pwa",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "generate-pwa-assets": "icon=$(find public -type f \\( -name 'icon.png' -o -name 'icon.jpg' -o -name 'icon.jpeg' -o -name 'icon.webp' -o -name 'icon.svg' \\) | head -n 1) && pwa-assets-generator --preset minimal $icon",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.10",
    "@mui/lab": "^5.0.0-alpha.166",
    "@mui/material": "^5.15.10",
    "@reduxjs/toolkit": "^2.2.1",
    "antd": "^5.23.2",
    "axios": "^1.7.7",
    "framer-motion": "^11.11.11",
    "js-cookie": "^3.0.5",
    "moment": "^2.30.1",
    "npm": "^11.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.6.2",
    "react-redux": "^9.1.0",
    "react-youtube": "^10.1.0",
    "slick-carousel": "^1.8.1",
    "ua-parser-js": "^2.0.0",
    "uninstall": "^0.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/mixpanel-browser": "^2.49.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-slick": "^0.23.13",
    "@types/styled-components": "^5.1.34",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "vite-plugin-copy": "^0.1.6",
    "vite-plugin-imagemin": "^0.5.3",
    "vite-plugin-pwa": "^0.18.1"
  }
}
