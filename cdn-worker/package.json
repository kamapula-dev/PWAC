{
  "name": "cdn-worker",
  "version": "1.0.0",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --minify --format=iife --target=es2022 --charset=utf8 --tree-shaking=true --outfile=dist/index.js && npm run optimize",
    "optimize": "terser dist/index.js -o dist/index.js --compress unsafe_arrows,unsafe_methods,unsafe_proto --mangle --mangle-props regex=/^_/ --rename"
  },
  "devDependencies": {
    "@types/jszip": "^3.4.1",
    "wrangler": "^3.87.0",
    "esbuild": "^0.19.2",
    "terser": "^5.27.0"
  },
  "dependencies": {
    "jszip": "^3.10.1"
  }
}